{
  "name": "antd-v",
  "version": "6.1.4",
  "type": "module",
  "homepage": "https://tujianglin.github.io/antd-v-docs/",
  "scripts": {
    "dev": "vite",
    "build": "node build.mjs && npm run build:type",
    "build:type": "vue-tsc --project ./tsconfig.build.json --emitDeclarationOnly && tsc-alias -p tsconfig.build.json",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.js"
    },
    "./style.css": "./dist/index.css",
    "./locale/*": "./dist/components/locale/*.js"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@ant-design/colors": "^7.2.1",
    "@ant-design/fast-color": "^3.0.0",
    "@ant-design/icons-vue": "^7.0.1",
    "@ctrl/tinycolor": "^4.1.0",
    "@emotion/unitless": "^0.10.0",
    "@vueuse/components": "^13.5.0",
    "@vueuse/core": "^13.5.0",
    "clsx": "^2.1.1",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.18",
    "es-toolkit": "^1.41.0",
    "is-mobile": "^5.0.0",
    "scroll-into-view-if-needed": "^3.1.0",
    "tailwindcss": "^4.1.11",
    "throttle-debounce": "^5.0.2",
    "tw-animate-css": "^1.3.4"
  },
  "devDependencies": {
